      <!-- Start Sign Up -->
      <div class="row"> 
        <div class="col-md-4"></div>
        <div class="col-md-4">
          <br><br><br>
                                <!-- <div class="panel panel-default" < style="background-color:transparent !important;> -->
          <div id="login_panel" class="panel panel-default" >  
                                        <!-- <div class="panel-body" style="background-color:transparent !important;> -->
            <div class="panel-body">
              <form action="/"  method="post">
                <div class="row">

                  <div class="col-md-6">
                    <div class="form-group">
                      <input type="text" class="form-control input-sm" id="firstname" name='firstname' placeholder="First Name" required>
                    </div>
                  </div>
                  <div class="col-md-6">
                    <div class="form-group">
                      <input type="text" class="form-control input-sm" id="lastname" name='lastname' placeholder="Last Name">
                    </div>
                  </div>
                </div>
                <div class="form-group">
                  <input type="email" class="form-control input-sm" id="InputEmail1" name='InputEmail1' placeholder="Email">
                </div>
                <div class="form-group">
                  <input type="text" class="form-control input-sm" id="InputMobile" name='InputMobile' placeholder="Mobile Number">
                </div>
                
                <div class="row"> 
                  <div class="col-md-6">
                    <div class="form-group">
                      <select class="form-control input-sm" id="sel1" name='sell'>
                        <option>Male</option>
                        <option>Female</option>
                        <option>Special</option>
                      </select>
                    </div>
                  </div>
                  <div class="col-md-6">
                    <div class="form-group">
                      <input id="dp" name='dp' type="Date" class="form-control input-sm" placeholder="Date of birth">
                    </div> 
                  </div>
                </div>
                <div class="row"> 
                  <div class="col-md-6">
                    <div class="form-group">
                      <input type="password" class="form-control input-sm" id="InputPassword" name='InputPassword' placeholder="Password">
                    </div>
                  </div>
                  <div class="col-md-6">
                    <div class="form-group">
                      <input type="password" class="form-control input-sm" id="InputRe_enter_Password" placeholder="Re-Enter Password">
                    </div>
                  </div>
                </div>             

                <div class="btn-group btn-group-justified" role="group" aria-label="...">
                  <label id="checkbox_remember_me"><input id="checkboxstyle" type="checkbox" value=""> Agree with the terms and conditions.</label> 
                </div>
                <div class="btn-group btn-group-justified" role="group" aria-label="...">
                  <div class="btn-group" role="group">
                    <button type="submit" class="btn btn-primary btn-sm ">SignUp </button>
                  </div>
                </div>
              </form>
                                        </div>
                                  </div>
                        </div>
        <div class="col-md-4"></div>
      </div>
